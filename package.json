{
  "name": "email-comb",
  "version": "5.2.0",
  "description": "Remove unused CSS from email templates",
  "keywords": [
    "body",
    "css",
    "email",
    "from",
    "head",
    "inline",
    "remove",
    "styles",
    "uncss",
    "unused"
  ],
  "homepage": "https://codsen.com/os/email-comb/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/email-comb-noesm.git"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "main": "dist/email-comb.cjs.js",
  "module": "dist/email-comb.esm.js",
  "browser": "dist/email-comb.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "ci_test": "npm run build && npm run format && npm run unit",
    "dev": "rollup -c --dev",
    "devunittest": "npm run dev && c8 yarn run unit",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "echo 'no lect'",
    "lint": "eslint . --ext .js --ext .ts --fix --config \".eslintrc.json\" --quiet",
    "prettier": "prettier '*.{js,css,scss,vue,md,ts}' --write --loglevel silent",
    "republish": "npm publish || :",
    "pretest": "npm run build",
    "test": "npm run lint && npm run unit && npm run format",
    "unit": "c8 uvu test"
  },
  "c8": {
    "check-coverage": false,
    "exclude": [
      "test/**/*.*"
    ],
    "lines": 100
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "array-pull-all-with-glob": "^5.1.0",
    "html-crush": "^4.2.0",
    "lodash.intersection": "^4.4.0",
    "lodash.pullall": "^4.2.0",
    "lodash.uniq": "^4.5.0",
    "matcher": "^4.0.0",
    "ranges-apply": "^5.1.0",
    "ranges-push": "^5.1.0",
    "regex-empty-conditional-comments": "^1.11.0",
    "string-extract-class-names": "^6.1.0",
    "string-left-right": "^4.1.0",
    "string-match-left-right": "^7.1.0",
    "string-range-expander": "^2.1.0",
    "string-uglify": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "eslint-plugin-row-num": "^1.5.12",
    "eslint-plugin-test-num": "^1.5.16",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.pullall": "^4.2.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^15.6.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "c8": "^7.11.0",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.1",
    "rollup": "^2.49.0",
    "rollup-plugin-ascii": "^0.0.3",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "uvu": "^0.5.3"
  }
}
